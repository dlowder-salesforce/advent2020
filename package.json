{
  "name": "advent2020",
  "description": "CLI to run AdventOfCode 2020 solutions",
  "version": "0.0.0",
  "author": "Douglas Lowder @dlowder-salesforce",
  "bin": {
    "advent2020": "./bin/run"
  },
  "bugs": "https://github.com/dlowder-salesforce/advent2020/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^10",
    "nyc": "^14"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/dlowder-salesforce/advent2020",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "advent2020",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "dlowder-salesforce/advent2020",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "readme": "oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  }
}
